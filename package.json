{
    "name": "BLOG",
    "description": "A simple blog",
    "version": "0.1.0",
    "scripts": {
        "sapper": "sapper dev",
        "build": "sapper build --legacy",
        "export": "sapper export --legacy",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "run-p --race dev cy:run",
        "prep": "rm -rf cypress appveyor.yml cypress.json README.md",
        "reload": "touch src/routes/_layout.svelte",
        "scss": "sass --no-source-map -s compressed ./static/",
        "autoprefixer": "postcss -u autoprefixer -r ./static/global.css --no-map",
        "build:css": "run-s 'scss' 'autoprefixer' 'reload'",
        "watch:css": "onchange './static/**/*.scss' -- npm run build:css",
        "dev": "run-p 'sapper' 'watch:css'"
    },
    "dependencies": {
        "bufferutil": "^4.0.1",
        "compression": "1.7.4",
        "fetch": "^1.1.0",
        "node-fetch": "^2.6.0",
        "notifit2": "^1.0.0",
        "parse": "^2.13.0",
        "polka": "0.5.2",
        "sirv": "0.4.2",
        "utf-8-validate": "^5.0.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-syntax-dynamic-import": "7.8.3",
        "@babel/plugin-transform-runtime": "7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/runtime": "^7.9.6",
        "@beyonk/svelte-notifications": "^2.0.3",
        "@neuekit/reboot": "1.10.2",
        "@neuekit/utilities": "1.5.4",
        "@rollup/plugin-alias": "3.1.0",
        "@rollup/plugin-babel": "^5.0.0",
        "@rollup/plugin-commonjs": "11.0.2",
        "@rollup/plugin-node-resolve": "^7.1.3",
        "@rollup/plugin-replace": "^2.3.2",
        "autoprefixer": "^9.7.6",
        "cl-editor": "^2.0.0",
        "dotenv": "^8.2.0",
        "npm-run-all": "4.1.5",
        "onchange": "7.0.2",
        "postcss": "7.0.29",
        "postcss-cli": "7.1.1",
        "rollup": "^2.8.2",
        "rollup-plugin-svelte": "5.2.1",
        "rollup-plugin-terser": "5.3.0",
        "sapper": "0.27.12",
        "sass": "^1.26.5",
        "svelte": "^3.22.2",
        "svelte-preprocess": "^3.7.4"
    }
}